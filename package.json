{
  "name": "awaismanzoor",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "main": "index.js",
  "private": false,
  "engines": {
    "node": ">=12.13.0",
    "npm": ">=6.12.1",
    "yarn": ">=1.17.3"
  },
  "scripts": {
    "start": "node dist/server/index.js",
    "dev": "nodemon src/index.js --exec \"node -r dotenv/config -r babel-register\"",
    "start:debug": "cross-env DEBUG=Boilerplate:* yarn start",
    "lint": "esw ./server/*.js server config --color",
    "lint:watch": "yarn lint -- --watch",
    "commit": "npx git-cz",
    "precommit": "npm run lint && npm run test --colors",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest -- --watch",
    "test:coverage": "yarn test --coverage --watchAll=false",
    "clean": "rimraf dist",
    "build": "yarn run clean && mkdir dist && babel server -s -D -d dist/server --ignore server/**/*.test.js && babel config -s -D -d dist/config --ignore **/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "Awais Manzoor",
    "boilerplate"
  ],
  "dependencies": {
    "@hapi/joi": "^16.1.7",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "compression": "1.7.4",
    "cookie-parser": "1.4.4",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "4.17.3",
    "express-jwt": "5.3.1",
    "express-validation": "^1.0.3",
    "express-winston": "4.0.1",
    "helmet": "3.21.1",
    "http-status": "1.4.0",
    "joi": "^14.3.1",
    "jsonwebtoken": "8.5.1",
    "method-override": "^3.0.0",
    "mongoose": "5.7.5",
    "morgan": "1.9.1",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-register": "^6.26.0",
    "commitizen": "^4.0.3",
    "coveralls": "^3.0.7",
    "cross-env": "6.0.3",
    "cz-conventional-changelog": "3.0.2",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "^23.0.2",
    "eslint-watch": "6.0.1",
    "husky": "3.0.9",
    "jest": "^24.9.0"
  },
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "server/**/*.js",
      "server/*.js",
      "config/*.js"
    ]
  }
}
